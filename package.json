{
  "name": "git-squash-interface",
  "displayName": "git-squash-interface",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gitSquashCommits",
          "title": "Commits",
          "icon": "resources/git-squash.svg"
        }
      ]
    },
    "views": {
      "gitSquashCommits": [
        {
          "id": "gitSquashCommitsView",
          "name": "Commits",
          "type": "webview"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "gitSquashCommits.refresh",
          "when": "view == gitSquashCommitsView",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "gitSquashCommits.refresh",
        "title": "Rafra√Æchir",
        "icon": "$(refresh)"
      }
    ]
  },
  "activationEvents": [
    "onView:gitSquashCommitsView"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
